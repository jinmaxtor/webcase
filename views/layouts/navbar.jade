nav.header-navbar.navbar.navbar-with-menu.navbar-fixed-top.navbar-semi-dark.navbar-shadow
    .navbar-wrapper
        .navbar-header
            ul.nav.navbar-nav
                li.nav-item.mobile-menu.hidden-md-up.float-xs-left
                    a.nav-link.nav-menu-main.menu-toggle.hidden-xs(href='#')
                        i.ft-menu.font-large-1
                li.nav-item
                    a.navbar-brand(href='index-2.html')
                        img.brand-logo(alt='stack admin logo', src='/app-assets/images/logo/stack-logo-light.png')
                        h2.brand-text WebCase
                li.nav-item.hidden-sm-down.float-xs-right
                    a.nav-link.modern-nav-toggle(data-toggle='collapse')
                        i.toggle-icon.ft-toggle-right.font-medium-3.white(data-ticon='ft-toggle-right')
                li.nav-item.hidden-md-up.float-xs-right
                    a.nav-link.open-navbar-container(data-toggle='collapse', data-target='#navbar-mobile')
                        i.fa.fa-ellipsis-v
        .navbar-container.content.container-fluid
            #navbar-mobile.collapse.navbar-toggleable-sm
                ul.nav.navbar-nav
                    li.nav-item.hidden-sm-down
                        a.nav-link.nav-link-expand(href='#')
                            i.ficon.ft-maximize
                    li.nav-item.nav-search
                        a.nav-link.nav-link-search(href='#')
                            i.ficon.ft-search
                        .search-input
                            input.input(type='text', placeholder='Explore Stack...')
                if (isAuthenticated)
                    ul.nav.navbar-nav.float-xs-right
                        li.dropdown.dropdown-user.nav-item
                            a.dropdown-toggle.nav-link.dropdown-user-link(href='#', data-toggle='dropdown')
                                span.avatar.avatar-online
                                    img(src='/app-assets/images/portrait/small/avatar-s-1.png', alt='avatar')
                                    i
                                span#nombre.user-name= usuario.nombre
                            .dropdown-menu.dropdown-menu-right
                                a.dropdown-item(href='#')
                                    i.ft-user
                                    |  Edit Profile
                                a.dropdown-item(href='#')
                                    i.ft-mail
                                    |  My Inbox
                                a.dropdown-item(href='#')
                                    i.ft-check-square
                                    |  Task
                                a.dropdown-item(href='#')
                                    i.ft-message-square
                                    |  Chats
                                .dropdown-divider
                                a.dropdown-item(href='/logout')
                                    i.ft-power
                                    |  Logout
                else
                    p=hola
